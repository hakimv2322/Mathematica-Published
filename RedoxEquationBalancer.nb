  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FormBox[
       SubscriptBox["Cr", "2"],
       TraditionalForm], 
      SuperscriptBox[
       FormBox[
        SubscriptBox[
         StyleBox["O",
          FontSlant->"Plain"], "7"],
        TraditionalForm], 
       RowBox[{"-", "2"}]]}], "+", 
     RowBox[{
      FormBox[
       SubscriptBox[
        StyleBox["C",
         FontSlant->"Plain"], "2"],
       TraditionalForm], 
      FormBox[
       SubscriptBox[
        StyleBox["H",
         FontSlant->"Plain"], "4"],
       TraditionalForm], 
      StyleBox["O",
       FontSlant->"Plain"]}]}], 
    StyleBox["\[Rule]",
     FontSlant->"Plain"], 
    RowBox[{
     SuperscriptBox[
